		const topdistance = r.top
		const botdistance = window.innerHeight - r.bottom
	
		
		const t = (x) => `translate(${2*Math.sqrt(Math.max(0,-50-x))}px,0)`
		dot_top.style.transform = t(topdistance)
		dot_bot.style.transform = t(botdistance)

